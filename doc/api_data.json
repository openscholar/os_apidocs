[
  {
    "type": "get",
    "url": "/basic/:id",
    "title": "Endpoints",
    "version": "0.3.0",
    "name": "GetEndpoints",
    "group": "Basic",
    "permission": [
      {
        "name": "admin",
        "title": "Admin access rights needed.",
